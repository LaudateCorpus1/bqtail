[
  {
    "When": {
      "Prefix": "/data/case020",
      "Suffix": ".json"
    },
    "Async": true,
    "Dest": {
      "Table": "bqtail.dummy",
      "TransientDataset": "temp",
      "UniqueColumns": [
        "id"
      ]
    },
    "Batch": {
      "RollOver": true,
      "Window": {
        "DurationInSec": 15
      }
    },
    "OnSuccess": [
      {
        "Action": "call",
        "Request": {
          "BodyURL": "${parentURL}/body.txt",
          "URL": "$callURL",
          "Method": "POST",
          "Auth": true
        }
      },
      {
        "Action": "query",
        "Request": {
          "SQLURL": "${parentURL}/summary.sql",
          "Dest": "bqtail.summary"
        }
      },
      {
        "Action": "delete"
      }
    ]
  }
]