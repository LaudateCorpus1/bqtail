{
  "When": {
      "Prefix": "/data/case${parentIndex}",
    "Suffix": ".json"
  },
  "Async": true,
  "Dest": {
    "Table": "bqtail.dummy_v${parentIndex}",
    "Transient":{"Dataset": "temp"},
    "UniqueColumns": [
      "id"
    ]
  },
  "Batch": {
    "RollOver": true,
    "Window": {
      "DurationInSec": 15
    }
  },
  "OnSuccess": [
    {
      "Action": "call",
      "Request": {
        "URL": "$callURL",
        "Method": "POST",
        "BodyURL": "${parentURL}/body.txt",
        "Auth": true
      }
    },
    {
      "Action": "query",
      "Request": {
        "SQLURL": "${parentURL}/summary.sql",
        "Dest": "bqtail.summary_v${parentIndex}"
      }
    },
    {
      "Action": "delete"
    }
  ]
}
