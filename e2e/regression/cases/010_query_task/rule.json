[

  {
    "When": {
      "Prefix": "/data/case010",
      "Suffix": ".json"
    },
    "Dest": {
      "Table": "bqtail.dummy",
      "TransientDataset": "temp",
      "UniqueColumns": ["id"],
      "Transform": {
        "event_id": "$EventID"
      }
    },
    "Batch": {
      "RollOver": true,
      "Window": {
        "DurationInSec": 15
      }
    },
    "OnSuccess": [
      {
        "Action": "delete"
      },
      {
        "Action": "query",
        "Request": {
          "SQL": "SELECT '$EventID' AS event_id, SPLIT('$URLs', ',') AS uris, COUNT(1) AS row_count, CURRENT_TIMESTAMP() AS completed FROM $TempTable",
          "Dest": "bqtail.summary"
        }
      }
    ]
  }
]