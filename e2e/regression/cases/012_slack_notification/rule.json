[
  {
    "When": {
      "Prefix": "/data/case012",
      "Suffix": ".json"
    },
    "Dest": {
      "Table": "bqtail.wrong_dummy"
    },
    "OnFailure": [
      {
        "Action": "notify",
        "Request": {
          "Channels": [
            "#e2e"
          ],
          "From": "BqTail",
          "Title": "bqtail.wrong_dummy ingestion",
          "Message": "$Error",
          "Secret": {
            "URL": "gs://${config.Bucket}/Secrets/slack.json.enc",
            "Key": "BqTailRing/BqTailKey"
          }
        }
      }
    ]
  }
]