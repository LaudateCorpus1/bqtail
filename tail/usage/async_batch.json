{
  "ErrorURL": "gs://YYY/errors/",
  "BatchURL": "gs://e2e-data/batch/",
  "JournalURL": "gs://YYY/journal/",
  "DeferTaskURL": "gs://e2e-data/tasks/",
  "Routes": [
    {
      "When": {
        "Prefix": "/data/folder",
        "Suffix": ".json"
      },
      "Async": true,
      "Batch": {
        "Window": {
          "DurationInSec": 45
        }
      },
      "Dest": {
        "Table": "mydataset.mytable"
      },
      "OnSuccess": [
        {
          "Action": "delete"
        }
      ]
    }
  ]
}