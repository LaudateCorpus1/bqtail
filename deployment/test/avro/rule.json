[
  {
    "When": {
      "Prefix": "/xxxxx/",
      "Suffix": ".avro"
    },
    "Async": true,
    "Dest": {
      "Table": "myproject:mydataset.mytable",
      "TransientDataset": "temp"
    },
    "Batch": {
      "Window": {
        "DurationInSec": 60
      }
    },
    "OnSuccess": [
      {
        "Action": "delete"
      }
    ],
    "OnFailure": [
      {
        "Action": "move",
        "Request": {
          "DestURL": "gs://${opsBucket}/BqTail/Errors"
        }
      },
      {
        "Action": "notify",
        "Request": {
          "Channels": [
            "#e2e"
          ],
          "Title": "Failed to load $Source",
          "Message": "$Error"
        }
      }
    ],
    "Info": {
      "Workflow": "my worflow",
      "Description": "data ingestion",
      "ProjectURL": "xxxxxx",
      "LeadEngineer": "me@com"
    }
  }
]